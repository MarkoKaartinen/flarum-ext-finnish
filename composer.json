{
    "name": "markokaartinen/flarum-ext-finnish",
    "type": "flarum-extension",
    "description": "Finnish language pack.",
	"homepage": "https://github.com/markokaartinen/flarum-ext-finnish",
    "keywords": ["locale"],
    "license": "MIT",
	"authors": [
		{
			"name": "Marko Kaartinen",
			"email": "markokaartinen@gmail.com",
			"homepage": "http://www.markokaartinen.net"
		}
	],
	"support": {
        "issues": "https://github.com/markokaartinen/flarum-ext-finnish/issues",
        "source": "https://github.com/markokaartinen/flarum-ext-finnish"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Finnish",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#FFFFFF",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "fi",
            "title": "Finnish"
        }
    }
}
